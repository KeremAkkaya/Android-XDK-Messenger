<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="AddressBar" format="reference" />
    <declare-styleable name="AddressBar">
        <attr name="inputTextColor" />
        <attr name="inputTextTypeface" />
        <attr name="inputTextStyle" />
        <attr name="inputTextSize" />
        <attr name="inputUnderlineColor" />
        <attr name="inputCursorColor" />
        <attr name="listTextSize" />
        <attr name="listTextColor" />
        <attr name="listTextTypeface" />
        <attr name="listTextStyle" />
        <attr name="chipTypeface" />
        <attr name="chipStyle" />
        <attr name="avatarBackgroundColor" />
        <attr name="avatarBorderColor" />
        <attr name="avatarTextColor" />
        <attr name="avatarTextTypeface" />
        <attr name="avatarTextStyle" />
    </declare-styleable>

    <!-- UI Address Bar -->
    <attr name="inputTextSize" format="dimension|reference" />
    <attr name="inputTextColor" format="color|reference" />
    <attr name="inputTextTypeface" format="string|reference" />
    <attr name="inputTextStyle">
        <flag name="normal" value="0" />
        <flag name="bold" value="1" />
        <flag name="italic" value="2" />
    </attr>
    <attr name="listTextSize" format="dimension|reference" />
    <attr name="listTextColor" format="color|reference" />
    <attr name="listTextTypeface" format="string|reference" />
    <attr name="listTextStyle">
        <flag name="normal" value="0" />
        <flag name="bold" value="1" />
        <flag name="italic" value="2" />
    </attr>
    <attr name="chipTypeface" format="string|reference" />
    <attr name="chipStyle">
        <flag name="normal" value="0" />
        <flag name="bold" value="1" />
        <flag name="italic" value="2" />
    </attr>
</resources>